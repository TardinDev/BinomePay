# Configuration BinomePay - Preview/Staging
# Variables d'environnement pour les builds de test

# Mode preview - API de staging
EXPO_PUBLIC_MOCK_API=false
NODE_ENV=staging
EXPO_PUBLIC_DEV_MODE=false

# Configuration API Backend Staging
EXPO_PUBLIC_API_URL=https://api-staging.binomepay.com/v1

# Configuration Supabase Staging
EXPO_PUBLIC_SUPABASE_URL=https://your-staging-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-staging-anon-key

# Configuration Clerk Test
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your-staging-key

# Configuration des notifications (test)
EXPO_PUBLIC_PUSH_TOKEN=your-expo-push-token-staging

# Debug activ√© pour les tests
EXPO_PUBLIC_ENABLE_LOGGING=true
EXPO_PUBLIC_DEBUG_MODE=true